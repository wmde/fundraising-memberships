<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WMDE\Fundraising\MembershipContext\Domain\Model\FeeChange" table="membership_fee_changes">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uuid" type="string" column="uuid" length="36" unique="true"/>
    <field name="externalMemberId" type="integer" column="external_member_id"/>
    <field name="currentAmountInCents" type="integer" column="current_amount_in_cents"/>
    <field name="suggestedAmountInCents" type="integer" column="suggested_amount_in_cents"/>
    <field name="currentInterval" type="integer" column="current_interval"/>
    <field name="state" column="state" enum-type="FeeChangeState"/>
    <field name="exportDate" type="datetime" column="export_date" nullable="true"/>
    <field name="paymentId" type="integer" column="payment_id" nullable="false">
      <options>
        <option name="unsigned">true</option>
        <option name="default">0</option>
      </options>
    </field>
    <indexes>
      <index name="m_fc_identifier" columns="uuid"/>
      <index name="m_fc_payment_id" columns="payment_id"/>
    </indexes>
  </entity>
</doctrine-mapping>
