<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:gedmo="http://gediminasm.org/schemas/orm/doctrine-extensions-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="WMDE\Fundraising\MembershipContext\Domain\Model\ModerationReason" table="membership_moderation_reason">
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="moderationIdentifier" column="moderation_identifier" type="ModerationIdentifier" nullable="false">
        </field>
        <field name="source" column="source" type="string" length="32" nullable="false">
            <options>
                <option name="default"/>
            </options>
        </field>
        <indexes>
            <index name="mr_identifier" columns="moderation_identifier,source"/>
        </indexes>
    </entity>
</doctrine-mapping>